{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "width": 800,
  "height": 500,
  "title": "europe",
  "projection": {"type": "equalEarth"},
  "layer": [
    {
      "data": {
        "url": "https://raw.githubusercontent.com/Rosheen594/WEEK9-Homework/main/studio%20codes%20(week%208)%E7%9A%84%E5%89%AF%E6%9C%AC/2_symbol_map/js/output.json",
        "format": {"type": "topojson", "feature": "output"}
      },
      "mark": {"type": "geoshape", "fill": "lightgray", "stroke": "white"}
    },
    {
      "data": {
        "url": "https://raw.githubusercontent.com/Rosheen594/WEEK9-Homework/main/studio%20codes%20(week%208)%E7%9A%84%E5%89%AF%E6%9C%AC/2_symbol_map/data/MLW_Data.csv"
      },
      "mark": {"type": "circle", "tooltip": {"content": "data"}},
      "encoding": {
        "longitude": {"field": "longitude", "type": "quantitative"},
        "latitude": {"field": "latitude", "type": "quantitative"},
        "size": {
          "field": "BeachName",
          "type": "nominal",
          "scale": {"domain": [4, 8]}
        },
        "color": {
          "field": "EventDate",
          "type": "ordinal",
          "scale": {"scheme": "reds"}
        }
      }
    }
  ]
}